@article{samoylenko1996ijpsych,
author = {Samoylenko, Elena and McAdams, Stephen and Nosulenko, Valery},
title = {Systematic Analysis of Verbalizations Produced in Comparing Musical Timbres},
journal = {International Journal of Psychology},
volume = {31},
number = {6},
pages = {255-278},
doi = {10.1080/002075996401025},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1080/002075996401025},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1080/002075996401025},
abstract = {A method of verbal protocol analysis is presented in which verbal units used to compare sound events are extracted from the protocols and analysed from three general points of view: (1) their logical sense, (2) their stimulus-relatedness, and (3) their semantic content. At each level, several analysis steps progressively label the verbal units from hierarchically superior to inferior levels (with increasing differentiation as one descends the hierarchy). The technique shows great reliability across analyses by independent experts. It was applied to an investigation of timbre comparisons in which Russian nonmusician subjects rated the dissimilarity of timbres presented in pairs and described verbally their similarities and differences. The technique demonstrates an effectiveness in revealing differences in focus on perceptual attributes of timbre that contribute to inter- and intra-subject discrepancies in dissimilarity ratings. Further, as concerns identification of the sound sources producing the events, there is evidence that basic categories are very general in nature in these nonmusician subjects.},
year = {1996}
}

